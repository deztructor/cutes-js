<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
   <suite name="cutes-js-tests" domain="system">
       <description>Cutes-js automatic tests</description>
       <set name="unit-tests" feature="cutes-js tests">
           <description>Testing cutes-js modules</description>
           <case manual="false" name="unittests">
               <step>cd @TESTS_DIR@ &amp;&amp; ./run -s -l1</step>
           </case>
       </set>
   </suite>
</testdefinition>
